<% include ../partials/header_start %>
<% include ../partials/header_end %>

<!-- ============================================================== -->
<!-- Start right Content here -->
<!-- ============================================================== -->
<div class="wrapper">
  <div class="container">

    <div class="row m-t-2">
      <div class="col-xs-12 col-md-6 col-lg-6 col-xl-3">
        <div class="card-box tilebox-three">
          <div class="bg-icon pull-xs-left">
            <i class="icon-list"></i>
          </div>
          <div class="text-xs-right">
            <h6 class="text-primary text-uppercase m-b-15 m-t-10">Levantamiento</h6>
            <a href="/Levantamiento" class="btn btn-primary">Nuevo</a>
          </div>
        </div>
      </div>

      <div class="col-xs-12 col-md-6 col-lg-6 col-xl-3">
        <div class="card-box tilebox-three">
          <div class="bg-icon pull-xs-left">
            <i class="icon-paper-clip"></i>
          </div>
          <div class="text-xs-right">
            <h6 class="text-primary text-uppercase m-b-15 m-t-10">Liga documental</h6>
            <a href="/documental" class="btn btn-primary">Listado</a>
          </div>
        </div>
      </div>

      <div class="col-xs-12 col-md-6 col-lg-6 col-xl-3">
        <div class="card-box tilebox-three">
          <div class="bg-icon pull-xs-left">
            <i class=" icon-energy"></i>
          </div>
          <div class="text-xs-right">
            <h6 class="text-primary text-uppercase m-b-15 m-t-10">Sincronizar</h6>
            <a href="/Sincronizar" class="btn btn-primary">Comenzar</a>
          </div>
        </div>
      </div>

      <div class="col-xs-12 col-md-6 col-lg-6 col-xl-3">
        <div class="card-box tilebox-three">
          <div class="bg-icon pull-xs-left">
            <i class="icon-equalizer"></i>
          </div>
          <div class="text-xs-right">
            <h6 class="text-primary text-uppercase m-b-15 m-t-10">Configuracion</h6>
            <a href="/documental" class="btn btn-primary">Comenzar</a>
          </div>
        </div>
      </div>

      <div class="row">
        <div class="col-lg-4">
          <div class="card-box">
            <h4 class="text-dark  header-title m-t-0 m-b-30">Comparativa</h4>

            <div class="widget-chart text-xs-center">
              <input data-plugin="knob" data-width="150" data-height="150" data-linecap=round data-fgColor="#5d9cec" value="55" data-skin="tron" data-angleOffset="180" data-readOnly=true data-thickness=".15"/>

              <p class="text-muted font-13 m-t-30">Compulsa entre el total de activos inventariados entre el total de activos documentados.</p>

              <div class="row text-xs-center m-t-30">
                <div class="col-xs-6">
                  <h3 data-plugin="counterup" class="text-success">376</h3>
                  <p class="text-success">Inventariados</p>
                </div>
                <div class="col-xs-6">
                  <h3 data-plugin="counterup" class="text-primary">229</h3>
                  <p class="text-primary">Documentados</p>
                </div>
              </div>
            </div>
          </div>
        </div>

        <div class="col-lg-4">
          <div class="card-box">
            <h4 class="text-dark  header-title m-t-0 m-b-30">Comparativa</h4>

            <div class="widget-chart text-xs-center">
              <input data-plugin="knob" data-width="150" data-height="150" data-linecap=round data-fgColor="#5d9cec" value="<%= PorcDocSync %>" data-skin="tron" data-angleOffset="180" data-readOnly=true data-thickness=".15"/>

              <p class="text-muted font-13 m-t-30">Total de activos documentados entre el total de activos sincronizados a la plataforma.</p>

              <div class="row text-xs-center m-t-30">
                <div class="col-xs-6">
                  <h3 data-plugin="counterup" class="text-success"><%= Documentados %></h3>
                  <p class="text-success">Documentados</p>
                </div>
                <div class="col-xs-6">
                  <h3 data-plugin="counterup" class="text-primary"><%= Sincronizados %></h3>
                  <p class="text-primary">Sincronizados</p>
                </div>
              </div>
            </div>
          </div>
        </div>

        <div class="col-lg-4">
          <div class="card-box">
            <h4 class="text-dark header-title m-t-0 m-b-30">Ultima sincronizacion</h4>

            <div class="table-responsive">
              <table class="table table-bordered">
                <thead>
                  <tr>
                    <th class="text-xs-center">Concepto</th>
                    <th class="text-xs-center">Ultima sincronizacion</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <td class="text-xs-center">Catalogos</td>
                    <td class="text-xs-center"><%= FCatalogos.fecha %></td>
                  </tr>
                  <tr>
                    <td class="text-xs-center">Ubicaciones y Areas</td>
                    <td class="text-xs-center" ><%= FUbicaciones.fecha %></td>
                  </tr>
                  <tr>
                    <td class="text-xs-center">Informacion documental</td>
                    <td class="text-xs-center"><%= FActivos.fecha %></td>
                  </tr>
                </tbody>
              </table>
            </div>
          </div>
        </div>

      </div>
    </div>

    <!-- ============================================================== -->
    <!-- End Right content here -->
    <!-- ============================================================== -->

    <% include ../partials/footer_start %>
    <% include ../partials/footer_end %>
